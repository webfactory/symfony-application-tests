{
    "name": "webfactory/symfony-application-tests",
    "description": "Contains several generic test scenarios that can be applied to Symfony 2 applications.",
    "keywords": [
        "Symfony",
        "tests",
        "testing",
        "functional tests",
        "integration tests"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "webfactory GmbH",
            "email": "info@webfactory.de",
            "homepage": "http://www.webfactory.de",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^7.2.0"
    },

    "require-dev": {
        "doctrine/common": "^2.4",
        "jms/security-extra-bundle": "^1.5",
        "phpunit/phpunit": "^7.4.0",
        "sensio/framework-extra-bundle": "^2.3.0 | ^3.0.0 | ^4.0.0 | 5.0.0",
        "symfony/config": "^3.4|^4.0",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/event-dispatcher": "^3.4|^4.0",
        "symfony/finder": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/routing": "^3.4|^4.0",
        "symfony/templating": "^3.4|^4.0",
        "symfony/twig-bundle": "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0",
        "symfony/web-profiler-bundle": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0",
        "twig/twig": "^1.34|^2.0"
    },

    "autoload": {
        "psr-0": {
            "Webfactory\\Tests": "src/",
            "Webfactory\\Constraint": "src/",
            "Webfactory\\Util": "src/"
        }
    },

    "autoload-dev": {
        "psr-0": {
            "Webfactory\\TestBundle": "tests/_files/test-application/src"
        },
        "classmap": [
            "tests/_files/test-application/app/TestKernel.php",
            "tests/Webfactory/Constraint/_files"
        ],
        "files": [
            "tests/_files/test-application/app/bootstrap.php"
        ]
    }
}
